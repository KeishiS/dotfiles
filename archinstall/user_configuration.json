{
    "hostname": "arch-keishis-home",
    "custom-commands": [
        "git clone https://github.com/KeishiS/dotfiles && cd dotfiles && pacman --noconfirm -S - < pacman.list"
    ],
    "version": "2.8.1",
    "config_version": "2.8.1",
    "archinstall-language": "English",
    "bootloader": "Systemd-boot",
    "debug": false,
    "kernels": [
        "linux"
    ],
    "uki": false,
    "swap": true,
    "timezone": "Asia/Tokyo",
    "skip_ntp": false,
    "locale_config": {
        "kb_layout": "jp106",
        "sys_enc": "UTF-8",
        "sys_lang": "en_US"
    },
    "additional-repositories": [],
    "audio_config": {
        "audio": "pipewire"
    },
    "network_config": {
        "type": "nm"
    },
    "parallel downloads": 15,
    "profile_config": {
        "gfx_driver": "All open-source",
        "greeter": "sddm",
        "profile": {
            "custom_settings": {
                "Sway": {
                    "seat_access": "polkit"
                },
                "i3-wm": {}
            },
            "details": [
                "i3-wm",
                "Sway"
            ],
            "main": "Desktop"
        }
    },
    "mirror_config": {
        "custom_mirrors": [],
        "mirror_regions": {
            "Japan": [
                "http://ftp.jaist.ac.jp/pub/Linux/ArchLinux/$repo/os/$arch",
                "https://www.miraa.jp/archlinux/$repo/os/$arch",
                "https://mirrors.cat.net/archlinux/$repo/os/$arch",
                "https://ftp.jaist.ac.jp/pub/Linux/ArchLinux/$repo/os/$arch",
                "http://www.miraa.jp/archlinux/$repo/os/$arch",
                "http://mirrors.cat.net/archlinux/$repo/os/$arch",
                "http://ftp.tsukuba.wide.ad.jp/Linux/archlinux/$repo/os/$arch"
            ]
        }
    },
    "no_pkg_lookups": false,
    "ntp": true,
    "offline": false,
    "packages": [
        "git",
        "zsh"
    ],
    "script": "unattended",
    "silent": false,
    "skip_version_check": false
}
